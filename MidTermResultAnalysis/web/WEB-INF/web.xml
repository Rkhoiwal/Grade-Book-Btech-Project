<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>SignUp</servlet-name>
        <servlet-class>servletpackage.SignUp</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Servletcheck</servlet-name>
        <servlet-class>servletpackage.Servletcheck</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>welcome</servlet-name>
        <servlet-class>welcomeservlet.Welcome</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ForgetPassword</servlet-name>
        <servlet-class>servletpackage.ForgetPassword</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>sumethod</servlet-name>
        <servlet-class>servletmethod.sumethod</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProfileDao</servlet-name>
        <servlet-class>servletpackage.ProfileDao</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Servletcheck</servlet-name>
        <url-pattern>/first</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>welcome</servlet-name>
        <url-pattern>/second</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ForgetPassword</servlet-name>
        <url-pattern>/third</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>sumethod</servlet-name>
        <url-pattern>/su</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SignUp</servlet-name>
        <url-pattern>/sg</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProfileDao</servlet-name>
        <url-pattern>/ProfileDao</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>homepage.jsp</welcome-file>
    </welcome-file-list>
</web-app>
